@model IEnumerable<Library_Management_System_By_Nausif_AND_Mohsin.Models.Publisher>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Publisher_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Publisher_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.publisher_Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Publisher_Address)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Publisher_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Publisher_Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.publisher_Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Publisher_Address)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Publisher_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Publisher_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Publisher_ID })
        </td>
    </tr>
}

</table>
