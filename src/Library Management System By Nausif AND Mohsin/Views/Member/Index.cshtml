        <td>
            @Html.DisplayFor(modelItem => item.Member_Gender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member_DOB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member_Type.Member_Type_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Member_ID }) | @Html.ActionLink("Delete", "Delete", new { id=item.Member_ID })
        </td>
    </tr>
}

</table>
